{
  "name": "open-sam-backend",
  "version": "1.0.0",
  "description": "OpenSAM Backend - Express.js + TypeScript + MongoDB + Redis",
  "main": "dist/server.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:api": "ts-node-dev --respawn --transpile-only src/server-minimal.ts",
    "dev:turn": "ts-node-dev --respawn --transpile-only src/daemon/turn-processor.ts",
    "dev:daemon": "ts-node-dev --respawn --transpile-only src/daemon-unified.ts",
    "dev:auction": "ts-node-dev --respawn --transpile-only src/daemon/auction-processor.ts",
    "dev:cmd": "ts-node-dev --respawn --transpile-only src/daemon.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:turn": "node dist/daemon/turn-processor.js",
    "start:daemon": "node dist/daemon-unified.js",
    "start:auction": "node dist/daemon/auction-processor.js",
    "start:cmd": "node dist/daemon.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "check:console": "rg 'console\\.(log|error|warn|info|debug)' src/ --type ts || echo 'No console statements found'",
    "clean": "rm -rf dist node_modules package-lock.json",
    "create-admin": "ts-node scripts/create-admin.ts",
    "create-admin:js": "node scripts/create-admin.mjs",
    "init-cities": "ts-node scripts/init-cities.ts",
    "check-cities": "ts-node scripts/check-cities.ts",
    "verify-db-cities": "ts-node scripts/verify-db-cities.ts",
    "init-general-commands": "ts-node scripts/initialize-general-commands.ts",
    "check-lock": "ts-node scripts/check-lock.ts",
    "create-test-setup": "ts-node scripts/create-test-setup.ts",
    "update-server-name": "ts-node scripts/update-server-name.ts",
    "create-servers": "ts-node scripts/create-servers.ts",
    "validate:gin7": "ts-node scripts/validate-gin7-catalog.ts",
    "seed:gin7-map": "ts-node scripts/seed-gin7-map.ts",
    "seed:gin7-map:dry-run": "ts-node scripts/seed-gin7-map.ts --dry-run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "jest --testPathPattern=e2e"
  },
  "dependencies": {
    "@types/express-session": "^1.18.2",
    "@types/multer": "^2.0.0",
    "@types/seedrandom": "^3.0.8",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "axios": "^1.7.9",
    "bcrypt": "^6.0.0",
    "compression": "^1.7.4",
    "connect-mongo": "^5.1.0",
    "connect-redis": "^9.0.0",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.2",
    "glob": "^11.0.3",
    "helmet": "^8.0.0",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "nanoid": "^3.3.7",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "redis": "^5.10.0",
    "seedrandom": "^3.0.5",
    "socket.io": "^4.8.1",
    "sql.js": "^1.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.7.5",
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.9.1",
    "@types/node-cron": "^3.0.11",
    "@types/socket.io": "^3.0.1",
    "@types/supertest": "^6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "autocannon": "^8.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
