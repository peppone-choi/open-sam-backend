{
  "name": "open-sam-backend",
  "version": "1.0.0",
  "description": "Express.js + TypeScript + MongoDB + CQRS",
  "main": "dist/server.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/server.ts",
    "dev:daemon": "ts-node-dev --respawn --transpile-only src/daemon.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "start:daemon": "node dist/daemon.js",
    "typecheck": "tsc --noEmit",
    "generate:tiles": "ts-node src/scripts/generate-city-tiles.ts"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.19.2",
    "nanoid": "^5.1.6",
    "node-cache": "^5.1.2",
    "node-cron": "^4.2.1",
    "redis": "^5.9.0",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "yup": "^1.7.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/ioredis": "^4.28.10",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20.11.0",
    "@types/node-cache": "^4.1.3",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/uuid": "^10.0.0",
    "ts-morph": "^27.0.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
